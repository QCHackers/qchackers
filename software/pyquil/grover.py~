from pyquil.quil import Program
from pyquil.api import QPUConnection
from pyquil.gates import *


qpu = QPUConnection(device_name='19Q-Acorn')

grover = Program(
    H(1),
    H(2),
    S(1),
    H(2),
    CNOT(1, 2),
    H(2),
    S(1),
    S(2),
    H(1),
    H(2),
    X(1),
    X(2),
    H(2),
    CNOT(1,2),
    H(2),
    X(1),
    X(2),
    H(1),
    H(2),
    
    
)
print("Grover N=2 A=00")
result = qpu.run_and_measure(grover, [1, 2], 100)
print(result)

ghz_state = Program(H(0), H(1), X(2), CNOT(1, 2), CNOT(0, 2), H(0), H(1), H(2))
print("3Q GHZ State YYX-Measurement")
result = qpu.run_and_measure(ghz_state, [0, 1, 2], 100)
print(result)

